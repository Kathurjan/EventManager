<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="520.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Gui.Controller.UserPageController">
   <children>
      <TableView fx:id="eventTableView" layoutX="14.0" layoutY="70.0" onMouseClicked="#handleShopParticipants" prefHeight="345.0" prefWidth="380.0" AnchorPane.leftAnchor="15.0">
         <columns>
            <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="locationColumn" prefWidth="75.0" text="Location" />
            <TableColumn fx:id="dateColumn" prefWidth="99.0" text="Date" />
            <TableColumn fx:id="priceColumn" prefWidth="51.0" text="Price" />
            <TableColumn fx:id="startTimeColumn" prefWidth="79.0" text="Start time" />
         </columns>
      </TableView>
      <TableView fx:id="participantsTableView" layoutX="416.0" layoutY="70.0" prefHeight="345.0" prefWidth="220.0" AnchorPane.rightAnchor="14.0">
        <columns>
          <TableColumn fx:id="userFirstNameColumn" prefWidth="110.0" text="First Name" />
            <TableColumn fx:id="userLastNameColumn" prefWidth="110.0" text="Last Name" />
        </columns>
      </TableView>
      <Label layoutX="162.0" layoutY="14.0" text="Events">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Label layoutX="449.0" layoutY="14.0" text="Participants">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Button fx:id="logoutButton" layoutX="577.0" layoutY="471.0" mnemonicParsing="false" onAction="#logoutBTN" text="Log out" />
   </children>
</AnchorPane>
