<AnchorPane prefHeight="315.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Gui.Controller.LoginController">
   <children>
      <TextField fx:id="UserName" layoutX="14.0" layoutY="128.0" onKeyPressed="#loginAfterEnter" promptText="Username" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" />
