      <Button fx:id="loginBtn" layoutX="17.0" layoutY="235.0" mnemonicParsing="false" onAction="#signIn" prefHeight="25.0" prefWidth="149.0" text="Sign In" AnchorPane.leftAnchor="17.0" AnchorPane.rightAnchor="23.0" />
      <Button fx:id="closeButton" layoutX="17.0" layoutY="267.0" mnemonicParsing="false" onAction="#close" prefHeight="25.0" prefWidth="149.0" text="Exit" AnchorPane.leftAnchor="17.0" AnchorPane.rightAnchor="23.0" />
      <Text layoutX="51.0" layoutY="72.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Login Menu" AnchorPane.topAnchor="35.0">
         <font>
            <Font size="40.0" />
         </font>
      </Text>
      <Text layoutX="14.0" layoutY="125.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username" />
      <Text layoutX="14.0" layoutY="186.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password" />
