<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Gui.Controller.LoginController">
   <children>
      <TextField fx:id="UserName" layoutX="226.0" layoutY="139.0" promptText="UserName" />
      <PasswordField fx:id="PassWord" layoutX="226.0" layoutY="219.0" promptText="PassWord" />
      <Button layoutX="226.0" layoutY="274.0" mnemonicParsing="false" onAction="#signIn" prefHeight="25.0" prefWidth="149.0" text="Sign In" />
      <Button fx:id="closeButton" layoutX="228.0" layoutY="330.0" mnemonicParsing="false" onAction="#close" prefHeight="25.0" prefWidth="149.0" text="Cancel" />
   </children>
</AnchorPane>
